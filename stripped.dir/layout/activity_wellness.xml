<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:alpha="0.7"
    android:background="@drawable/boat_tied_to_pier"
    tools:context=".Wellness"
    android:visibility="visible">

    <TextView
        android:id="@+id/txtWellness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/wellness"
        android:textColor="#070707"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="350dp"
        android:layout_height="450dp"
        app:layout_constraintBottom_toTopOf="@id/btn_AddWellness"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txtWellness">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Wellness"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/wellness"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgFitness_Wellness"
                        android:layout_width="84dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/fitness_wellness_logo" />

                    <TextView
                        android:id="@+id/textWellness"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Wellness"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Atlantic_Gym"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/atlantic_strength"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgAtlantic_Gym"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/atlantic_strength_and_fitness" />

                    <TextView
                        android:id="@+id/textAtlantic_Gym"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Atlantic_Gym"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnAtlantic_GymFB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Atlantic Gym FB" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Comhcheol"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/comhcheol_harmony"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgComhcheol"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/comhcheol" />

                    <TextView
                        android:id="@+id/textComhcheol"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Comhcheol"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnComhcheol"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Comhcheol - Inspiration" />

                    <Button
                        android:id="@+id/btnDgl_Yoga_Retreats"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Donegal Yoga Retreats" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Wholistic_Healing"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/wholistic_house"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgWholisticHealing"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/wholistic_house_of_healing" />

                    <TextView
                        android:id="@+id/textWholistic_Healing"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Wholistic_Healing"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnWholistic_Healing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Wholistic House of Healing" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Soul_Tree"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/soul_tree"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgSoul_Tree"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/soul_tree" />

                    <TextView
                        android:id="@+id/textSoul_Tree"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Soul_Tree"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnSoulTreeFB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Soul Tree Studio FB" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/Social_Prescribing"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/social_prescribing"
                        android:textColor="#9C27B0"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/imgSocial_Prescribing"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/social_prescribing" />

                    <TextView
                        android:id="@+id/textSocial_Prescribing"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Social_Prescribing"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnSocial_Prescribing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Social Prescribing" />

                    <Button
                        android:id="@+id/btnSocial_Prescribing2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="Social Prescribing at Ionad Naomh Padraig" />


                </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btn_AddWellness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="@color/colorPrimary"
        android:gravity="center"
        android:text="@string/add_wellness"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintBottom_toTopOf="@id/btnAdditionalSvcs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <Button
        android:id="@+id/btnAdditionalSvcs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:backgroundTint="@color/colorPrimary"
        android:text="@string/additional_svcs"
        android:textSize="18sp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_AddWellness"
        app:layout_constraintBottom_toTopOf="@id/btnHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/btnHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="@color/colorPrimary"
        android:gravity="center"
        android:text="Home"
        android:textSize="18sp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAdditionalSvcs" />

</androidx.constraintlayout.widget.ConstraintLayout>