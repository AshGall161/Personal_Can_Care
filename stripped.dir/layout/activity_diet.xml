<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Diet"
    android:background="@drawable/boat_tied_to_pier"
    android:visibility="visible"
    tools:visibility="visible">

    <TextView
        android:id="@+id/txtOtherServices"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/diet_food"
        android:textColor="#070707"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="350dp"
        android:layout_height="450dp"
        app:layout_constraintBottom_toTopOf="@id/btnAdditionalSvcs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtOtherServices">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtFood"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="Food"
                        android:textColor="#9C27B0"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/imgDiet"
                        android:layout_width="84dp"
                        android:layout_height="76dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/diet" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Diet"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/CaifeKitty"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/caife_kitty"
                        android:textColor="#9C27B0"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/imgCaife_Kitty"
                        android:layout_width="74dp"
                        android:layout_height="66dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/caife_kitty" />

                    <ImageView
                        android:id="@+id/imgCaife_Kitty2"
                        android:layout_width="74dp"
                        android:layout_height="66dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/caife_kitty2" />

                    <TextView
                        android:id="@+id/textCaifeKitty"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/CaifeKitty"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnCaifeKittyFB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="@string/caife_kitty" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/CillUlta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/cillulta"
                        android:textColor="#9C27B0"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/imgCillUlta"
                        android:layout_width="74dp"
                        android:layout_height="66dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/cillulta" />

                    <TextView
                        android:id="@+id/textCillUlta"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/CillUlta"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnCillUlta"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="@string/cillulta" />

                    <Button
                        android:id="@+id/btnCillUltaFB"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="@string/cillulta_fb" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/LovingIt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/lovin_it_vegan"
                        android:textColor="#9C27B0"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/imgLovin_It_Vegan"
                        android:layout_width="74dp"
                        android:layout_height="66dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/lovin_it_vegan" />

                    <TextView
                        android:id="@+id/textLovingItVegan"
                        android:layout_width="316dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp"
                        android:text="@string/Loving_It_Vegan"
                        android:textColor="#2E2A2A"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/btnLovingItVegan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="15dp"
                        android:backgroundTint="@color/colorPrimary"
                        android:onClick="browser1"
                        android:text="@string/lovin_it_vegan" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btnAddFood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="@color/colorPrimary"
        android:gravity="center"
        android:text="@string/diet_add"
        android:textSize="16sp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintBottom_toTopOf="@id/btnAdditionalSvcs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <Button
        android:id="@+id/btnAdditionalSvcs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorPrimary"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/additional_svcs"
        android:textSize="16sp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/btnAddFood"
        app:layout_constraintBottom_toTopOf="@id/btnHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <Button
        android:id="@+id/btnHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="@color/colorPrimary"
        android:gravity="center"
        android:text="Home"
        android:textSize="16sp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/btnAdditionalSvcs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>